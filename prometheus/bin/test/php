# lion
use mysql;
CREATE DATABASE lion CHARACTER SET utf8 COLLATE utf8_general_ci;
CREATE USER 'lion' IDENTIFIED BY 'lion';
GRANT ALL ON lion.* TO 'lion'@'%' IDENTIFIED BY 'ff3b67c3b';
GRANT ALL ON lion.* TO 'lion'@'localhost' IDENTIFIED BY 'ff3b67c3b';
FLUSH PRIVILEGES;

