# Firewall configuration written by system-config-firewall
# Manual customization of this file is not recommended.
*filter
:INPUT ACCEPT [0:0]
:FORWARD ACCEPT [0:0]
:OUTPUT ACCEPT [0:0]
-A INPUT -m state --state ESTABLISHED,RELATED -j ACCEPT
-A INPUT -p icmp -j ACCEPT
-A INPUT -i lo -j ACCEPT

# sshd
-I INPUT -p tcp --dport 22 -j DROP
-I INPUT -s 192.168.111.119/32 -p tcp --dport 22 -j ACCEPT
-I INPUT -s 192.168.111.121/32 -p tcp --dport 22 -j ACCEPT
-I INPUT -s 192.168.111.122/32 -p tcp --dport 22 -j ACCEPT
-I INPUT -s 172.16.15.176/32 -p tcp --dport 22 -j ACCEPT
-I INPUT -s 172.16.24.170/32 -p tcp --dport 22 -j ACCEPT

# pureftpd
-A INPUT -m state --state NEW -m tcp -p tcp --dport 20:21 -j ACCEPT
-A INPUT -m state --state NEW -m tcp -p tcp --dport 10021:10026 -j ACCEPT

# nginx
-A INPUT -m state --state NEW -m tcp -p tcp --dport 80 -j ACCEPT
-A INPUT -m state --state NEW -m tcp -p tcp --dport 443 -j ACCEPT
# zabbix_agentd
-A INPUT -m state --state NEW -m tcp -p tcp --dport 10050 -j ACCEPT

# tomcat & jmx
-A INPUT -m state --state NEW -m tcp -p tcp --dport 8080:8089 -j ACCEPT
#-A INPUT -m state --state NEW -m tcp -p tcp --dport 10000:10009 -j ACCEPT
-I INPUT -p tcp --dport 10000:10009 -j DROP
-I INPUT -s 10.0.0.0/8 -p tcp --dport 10000:10009 -j ACCEPT
#-A INPUT -m state --state NEW -m tcp -p tcp --dport 10100:10109 -j ACCEPT
-I INPUT -p tcp --dport 10100:10109 -j DROP
-I INPUT -s 10.0.0.0/8 -p tcp --dport 10100:10109 -j ACCEPT

# memcached
-A INPUT -m state --state NEW -m tcp -p tcp --dport 11211 -j ACCEPT

# redis
-A INPUT -m state --state NEW -m tcp -p tcp --dport 6379  -j ACCEPT

# activemq
-A INPUT -m state --state NEW -m tcp -p tcp --dport 1099 -j ACCEPT
-A INPUT -m state --state NEW -m tcp -p tcp --dport 1616 -j ACCEPT

# mysql
-A INPUT -m state --state NEW -m tcp -p tcp --dport 3306 -j ACCEPT

-A INPUT -j REJECT --reject-with icmp-host-prohibited
-A FORWARD -j REJECT --reject-with icmp-host-prohibited
COMMIT



