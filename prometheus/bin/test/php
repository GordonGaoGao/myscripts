#!/bin/bash

sudo bash
cat << EOF > /etc/sysconfig/iptables
# Firewall configuration written by system-config-firewall
# Manual customization of this file is not recommended.
*filter
:INPUT ACCEPT [0:0]
:FORWARD ACCEPT [0:0]
:OUTPUT ACCEPT [0:0]
-A INPUT -m state --state ESTABLISHED,RELATED -j ACCEPT
-A INPUT -p icmp -j ACCEPT
-A INPUT -i lo -j ACCEPT

# intral
-A INPUT -s 10.0.0.0/8 -j ACCEPT

# sshd
-I INPUT -p tcp --dport 22 -j DROP
-I INPUT -s 115.236.161.16/28 -p tcp --dport 22 -j ACCEPT
-I INPUT -s 124.160.28.144/29 -p tcp --dport 22 -j ACCEPT
-I INPUT -s 10.175.201.227/32 -p tcp --dport 22 -j ACCEPT
-I INPUT -s 10.168.104.44/32 -p tcp --dport 22 -j ACCEPT
-I INPUT -s 10.168.252.244/32 -p tcp --dport 22 -j ACCEPT
-I INPUT -s 10.173.138.25/32 -p tcp --dport 22 -j ACCEPT

# web
-A INPUT -m state --state NEW -m tcp -p tcp --dport 80 -j ACCEPT
-A INPUT -m state --state NEW -m tcp -p tcp --dport 443 -j ACCEPT
-A INPUT -m state --state NEW -m tcp -p tcp --dport 8080:8089 -j ACCEPT

# debug
-I INPUT -p tcp --dport 9999 -j DROP
-I INPUT -s 115.236.161.16/28 -p tcp --dport 9999 -j ACCEPT
-I INPUT -s 124.160.28.144/29 -p tcp --dport 9999 -j ACCEPT

-A INPUT -j REJECT --reject-with icmp-host-prohibited
-A FORWARD -j REJECT --reject-with icmp-host-prohibited
COMMIT
EOF

chkconfig iptables on
service iptables stop
service iptables start6666666666666666666666



